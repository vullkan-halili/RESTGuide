{
  "name": "restguide",
  "version": "1.0.0",
  "description": "An opinionated guide on how to build a RESTful API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"./src/**/*.js\"",
    "test:modules": "NODE_ENV=test mocha --require @babel/register --timeout 30000 --exit \"./src/app/modules/**/*.test.js*\"",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register --timeout 30000 --exit \"./src/**/*.test.js*\"",
    "test:only": "cross-env NODE_ENV=test mocha --timeout 30000 --exit --require @babel/register $1",
    "test:ci": "npm run test",
    "start": "nodemon --exec babel-node ./src/index.js",
    "build": "babel src --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeroabsolute/RESTGuide.git"
  },
  "keywords": [
    "REST",
    "API",
    "guide",
    "Node.js",
    "Express.js"
  ],
  "author": "Gerald Haxhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeroabsolute/RESTGuide/issues"
  },
  "homepage": "https://github.com/zeroabsolute/RESTGuide#readme",
  "dependencies": {
    "@hapi/joi-date": "^2.0.1",
    "@sendgrid/mail": "^7.3.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "cuid": "^2.1.8",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-bunyan-logger": "^1.3.3",
    "faker": "^5.1.0",
    "inline-css": "^3.0.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "minio": "^7.0.16",
    "mongoose": "^5.10.11",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.4.4",
    "sinon": "^9.2.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.1.4",
    "web-push": "^3.4.4"
